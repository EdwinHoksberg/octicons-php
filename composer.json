{
    "name": "edwinhoksberg/octicons-php",
    "description": "A simple PHP library for displaying the Github Octicons font icons in svg format.",
    "keywords": ["octicons", "github", "icons", "svg"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edwin Hoksberg",
            "email": "mail@edwinhoksberg.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Octicons\\": "src/"
        }
    },
    "require": {
        "php": ">= 7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0",
        "twig/twig": "^2.10|^3.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "suggest": {
        "twig/twig": "A helper function is available for twig for creating icons easily in twig templates."
    }
}
